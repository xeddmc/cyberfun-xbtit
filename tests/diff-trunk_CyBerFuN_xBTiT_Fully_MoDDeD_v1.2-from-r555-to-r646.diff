+
+          }
+
+        break;
+
+
+
+    case 'delete':
+
+        if (isset($_GET["id"]))
+
+          {
+
+           // we should get only 1 style, selected with radio ...
+
+           $id=max(0,$_GET["id"]);
+
+           // delete style from database
+
+           do_sqlquery("DELETE FROM {$TABLE_PREFIX}warn_reasons WHERE id=$id",true);
+
+           category_read();
+
+          }
+
+        break;
+
+
+
+            
+
+    case '':
+
+    case 'read':
+
+    default:
+
+      warn_read();
+
+}
